<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManageMenuController">
   <children>
      <TextField fx:id="txtItem" layoutX="56.0" layoutY="104.0" prefHeight="35.0" prefWidth="184.0" promptText="e.g. wild seabass ">
         <font>
            <Font name="Inconsolata" size="13.0" />
         </font></TextField>
      <TextField fx:id="txtPrice" layoutX="250.0" layoutY="104.0" prefHeight="35.0" prefWidth="89.0" promptText="10">
         <font>
            <Font name="Inconsolata" size="13.0" />
         </font></TextField>
      <Text layoutX="56.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Menu">
         <font>
            <Font name="Inconsolata" size="36.0" />
         </font>
      </Text>
      <Button fx:id="btnAdd" layoutX="57.0" layoutY="151.0" mnemonicParsing="false" onAction="#addNewItem" prefHeight="27.0" prefWidth="66.0" text="add" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="90.0" layoutY="250.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Desktop/egg-40292_960_720.png" />
         </image>
      </ImageView>
      <TableView fx:id="tvItems" layoutX="435.0" layoutY="44.0" prefHeight="386.0" prefWidth="421.0">
        <columns>
          <TableColumn fx:id="itemName" prefWidth="310.0" text="Item Name" />
          <TableColumn fx:id="itemPrice" prefWidth="105.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="btnDelete" layoutX="435.0" layoutY="441.0" mnemonicParsing="false" onAction="#deleteItem" text="Delete Item" />
      <Button layoutX="63.0" layoutY="441.0" mnemonicParsing="false" onAction="#home" text="Home" />
   </children>
</AnchorPane>
